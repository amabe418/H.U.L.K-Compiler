// Test para verificar que el error del phi node se ha corregido

type Point(x, y) {
    x = x;
    y = y;
    
    getX() => self.x;
    getY() => self.y;
};

// Test con if que puede causar el error del phi node
let p1 = new Point(3, 4) in {
    let p2 = new Point(5, 6) in {
        let result = if (3 > 2) then p1 else p2 in {
            print("Result X: " @ result.getX());
            print("Result Y: " @ result.getY());
        };
    };
}; 