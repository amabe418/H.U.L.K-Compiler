type Animal {
    method speak() : String {
        "Animal speaks"
    }
};

type Dog inherits Animal {
    method speak() : String {
        "Dog barks"
    }
};

type Cat inherits Animal {
    method speak() : String {
        "Cat meows"
    }
};

function test_polymorphism() : String {
    let animal:Animal = new Dog() in {
        animal.speak()
    }
};

test_polymorphism() 