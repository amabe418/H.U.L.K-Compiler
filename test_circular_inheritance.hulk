// Test de prevención de herencia circular
// Este ejemplo debería ser rechazado por el compilador

// Clase base
type Base() {
    method baseMethod() {
        print("Base method");
    }
}

// Clase que hereda de Base
type Derived() {
    base();
    
    method derivedMethod() {
        print("Derived method");
    }
}

// Intentar crear herencia circular (esto debería fallar)
// type Circular() {
//     base();  // Esto intentaría heredar de sí misma
// }

// Función para probar la herencia normal
function testNormalInheritance() {
    let obj = new Derived();
    obj.baseMethod();    // Método heredado
    obj.derivedMethod(); // Método propio
}

testNormalInheritance(); 